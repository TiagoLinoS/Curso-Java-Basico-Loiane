package com.tiago.cursojava.aula11;

public class VariaveisChar {

	public static void main(String[] args) {

		//O char usa a tabela de caracteres ASCII, sendo cada caracter um n√∫mero.
		/*char o = 'o';
		char i = 'i';*/
		char o = 111;
		char i = 105;
		
		char interrogacao = 0X00E1; // valor '?'
		
		System.out.println(""+o+i+interrogacao);

	}

}
